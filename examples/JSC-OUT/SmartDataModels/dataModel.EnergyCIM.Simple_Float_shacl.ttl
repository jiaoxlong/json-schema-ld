@prefix sdmshape: <https://w3id.org/sdm/shapes/schema.json#>.

<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/dataModels.CIMEnergyClasses/Simple_Float/schema.json>;
    sh:property [
  sh:path sdm:type;
  sh:description "Property. NGSI type. It has to be Simple_Float";
  sh:name "type";
  sh:in (sdm:Simple_Float)
], [
  sh:path sdm:value;
  sh:description "Property. Model:'https://schema.org/Number'.  Default: 0.0";
  sh:name "value";
  sh:datatype xsd:decimal
].
