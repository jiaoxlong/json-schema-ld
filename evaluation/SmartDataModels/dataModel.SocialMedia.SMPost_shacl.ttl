@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <https://w3id.org/gbfs#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix mobivoc: <http://schema.mobivoc.org/#>.
@prefix sdm: <https://w3id.org/sdm#>.
@prefix sdmshape: <https://w3id.org/sdm/shapes/schema.json#>.

<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://raw.githubusercontent.com/smart-data-models/dataModel.SocialMedia/master/SMPost/schema.json>;
    sh:property [
  sh:path sdm:type;
  sh:description "Property. NGSI-LD Entity Type. It must be equal to SMPost.";
  sh:name "type";
  sh:in (sdm:SMPost);
  sh:minCount 1;
  sh:maxCount 1
], [
  sh:path sdm:postCreatedAt;
  sh:description "Property. The datetime of the creation of the SMPost.";
  sh:name "postCreatedAt";
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype xsd:dateTime
], [
  sh:path sdm:postId;
  sh:description "Property.  Model:'https://schema.org/Text'. The  post ID of the SMPost.";
  sh:name "postId";
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype xsd:string
], [
  sh:path sdm:hasLanguage;
  sh:description "Property.  Model:'https://schema.org/Text'. The language of the post.";
  sh:name "hasLanguage";
  sh:datatype xsd:string
], [
  sh:path sdm:hasPostURL;
  sh:description "Property.  Model:'https://schema.org/Text'. The URL of the post.";
  sh:name "hasPostURL";
  sh:datatype xsd:string
], [
  sh:path sdm:hasText;
  sh:name "hasText";
  sh:datatype xsd:string
], [
  sh:path sdm:hasImages;
  sh:name "hasImages";
  sh:datatype xsd:string
], [
  sh:path sdm:hasVideos;
  sh:name "hasVideos";
  sh:datatype xsd:string
], [
  sh:path sdm:hasPrivacyLevel;
  sh:description "Property.  Model:'https://schema.org/Text'. The privacy setting of the post.";
  sh:name "hasPrivacyLevel";
  sh:datatype xsd:string
], [
  sh:path sdm:hasHashtags;
  sh:name "hasHashtags";
  sh:datatype xsd:string
], [
  sh:path sdm:hasThumbnails;
  sh:name "hasThumbnails";
  sh:datatype xsd:string
], [
  sh:path sdm:platform;
  sh:description "Property. Platform of post.";
  sh:name "platform";
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype xsd:string
], [
  sh:path sdm:createdBy;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:hasReferencedLocations;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:hasMentions;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:hasAnalysis;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:hasInteractionCount;
  sh:description "Property. The different interactions of this post.";
  sh:name "hasInteractionCount"
].
sdm:HasInteractionCountShape rdf:type sh:NodeShape;
    sh:targetClass sdm:HasInteractionCount;
    sh:property [
  sh:path sdm:interactionType;
  sh:name "interactionType";
  sh:in (sdm:Comment sdm:Dislike sdm:Favorite sdm:Like sdm:Quote sdm:Reactions sdm:Reply sdm:Retweet sdm:Shares sdm:Views)
], [
  sh:path sdm:count;
  sh:name "count";
  sh:datatype xsd:decimal
], [
  sh:path sdm:belongsToCollection;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
].
