@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <https://w3id.org/gbfs#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix mobivoc: <http://schema.mobivoc.org/#>.
@prefix sdm: <https://w3id.org/sdm#>.
@prefix sdmshape: <https://w3id.org/sdm/shapes/schema.json#>.

<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/dataModel.Aeronautics/AircraftModel/schema.json>;
    sh:property [
  sh:path sdm:type;
  sh:description "Property. NGSI Entity type. It has to be AircraftModel";
  sh:name "type";
  sh:in (sdm:AircraftModel);
  sh:minCount 1;
  sh:maxCount 1
], [
  sh:path sdm:codeIATA;
  sh:description "Property. Model:'http://schema.org/Text'. IATA aircraft type";
  sh:name "codeIATA";
  sh:pattern "^[A-Z0-9]{3}$";
  sh:datatype xsd:string
], [
  sh:path sdm:codeICAO;
  sh:description "Property. Model:'http://schema.org/Text'. ICAO aircraft type";
  sh:name "codeICAO";
  sh:pattern "^[A-Z]{1}[A-Z0-9]{3}$";
  sh:datatype xsd:string
], [
  sh:path sdm:length;
  sh:description "Property. Model:'http://schema.org/Number'. Aircraft model length in metres. Units:'metres'";
  sh:name "length";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:wingSpan;
  sh:description "Property. Model:'http://schema.org/Number'. Aircraft model wingspan in metres. Units:'metres'";
  sh:name "wingSpan";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:height;
  sh:description "Property. Model:'http://schema.org/Number'. Aircraft model height in metres. Units:'metres'";
  sh:name "height";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:mtow;
  sh:description "Property. Model:'http://schema.org/Number'.  Aircraft maximum takeoff weight in kilograms. Units:'kilograms'";
  sh:name "mtow";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:maximumAllowedSpeed;
  sh:description "Property. Model:'http://schema.org/Number'.  Aircraft maximum speed in kilometers per hour. Units:'kilometers per hour'";
  sh:name "maximumAllowedSpeed";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:maximumAllowedFuel;
  sh:description "Property. Model:'http://schema.org/Number'. Aircraft maximum fuel in kilograms. Units:'kilograms'";
  sh:name "maximumAllowedFuel";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:ceiling;
  sh:description "Property. Model:'http://schema.org/Number'. Maximum altitude the aircraft model can reach in metres. Units:'metres'";
  sh:name "ceiling";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:numberOfEngines;
  sh:description "Property. Model:'http://schema.org/Integer'. Number of engines";
  sh:name "numberOfEngines";
  sh:minInclusive 0;
  sh:datatype xsd:integer
], [
  sh:path sdm:capacity;
  sh:description "Property. Model:'http://schema.org/Integer'. Number of seatings";
  sh:name "capacity";
  sh:minInclusive 0;
  sh:datatype xsd:integer
].
