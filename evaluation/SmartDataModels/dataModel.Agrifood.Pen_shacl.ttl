@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <https://w3id.org/gbfs#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix mobivoc: <http://schema.mobivoc.org/#>.
@prefix sdm: <https://w3id.org/sdm#>.
@prefix sdmshape: <https://w3id.org/sdm/shapes/schema.json#>.

<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/dataModel.Agrifood/Pen/schema.json>;
    sh:property [
  sh:path sdm:type;
  sh:description "Property. NGSI Entity type. it has to be Pen";
  sh:name "type";
  sh:in (sdm:Pen);
  sh:minCount 1;
  sh:maxCount 1
], [
  sh:path sdm:additionalInfo;
  sh:description "Property. list of all the raw values sent by the sensor/platform with all the possible extra properties that are not included in the main structure. It is a JSON structure similar to this: {'name': 'temperature', 'value' : 32}";
  sh:name "additionalInfo"
].
sdm:AdditionalInfoShape rdf:type sh:NodeShape;
    sh:targetClass sdm:AdditionalInfo;
    sh:property [
  sh:path sdm:name;
  sh:name "name";
  sh:datatype xsd:string
], [
  sh:path sdm:value;
  sh:or ([ sh:datatype xsd:string ] [ sh:datatype xsd:decimal ] [ sh:datatype xsd:boolean ])
], [
  sh:path sdm:arrivalTimestamp;
  sh:description "Property. Model:'https://schema.org/DateTime'. Date and Time for the arrival of animals to the Pen";
  sh:name "arrivalTimestamp";
  sh:datatype xsd:dateTime
], [
  sh:path sdm:avgGrowth;
  sh:description "Property. Model:'https://schema.org/Number'. The average growth in weight of the animal in this pen";
  sh:name "avgGrowth";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:avgWeight;
  sh:description "Property. Units:'Kg'. Model:'https://schema.org/Number'. The average weight of the animal in this Pen. ";
  sh:name "avgWeight";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:buildingId;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier with format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity."
])
], [
  sh:path sdm:co2;
  sh:description "Property. Model:'https://schema.org/Number'. The CO2 concentration in the item";
  sh:name "co2";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:companyId;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:compartmentId;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:deadAnimalsSinceDateOfArrival;
  sh:description "Property. Model:'https://schema.org/Number'. Number of dead animals since the date of arrival";
  sh:name "deadAnimalsSinceDateOfArrival";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:farmId;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Identifier format of any NGSI entity"
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Identifier format of any NGSI entity"
])
], [
  sh:path sdm:feedConsumption;
  sh:description "Property. Model:'https://schema.org/Number'. The total amount of food that has been eaten from the feeding station(s) in the pen.It is measured through feed intakes and specific structure to let only one animal at a time to eat. Units:'Kg'";
  sh:name "feedConsumption";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:humidity;
  sh:description "Property. Model:'https://schema.org/Number'. Quantity representing the amount of water vapour in the atmosphere in the pen. ";
  sh:name "humidity";
  sh:minInclusive 0;
  sh:maxInclusive 1;
  sh:datatype xsd:decimal
], [
  sh:path sdm:lastUpdate;
  sh:description "Property. Model:'https://schema.org/DateTime'. Date and time at which the measurements in the item were taken. Units:'Seconds'";
  sh:name "lastUpdate";
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype xsd:dateTime
], [
  sh:path sdm:luminosity;
  sh:description "Property. Model:'https://schema.org/Number'. The brightness of a light source of a certain wavelength at the item. Units:'LUX'";
  sh:name "luminosity";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:numAnimals;
  sh:description "Property. Model:'https://schema.org/Number'. Number of animals contained in the Pen.";
  sh:name "numAnimals";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:sex;
  sh:description "Property. Model:'https://schema.org/Number'. The sex of the animals contained in the pen";
  sh:name "sex";
  sh:in (sdm:M sdm:F sdm:unknown sdm:)
], [
  sh:path sdm:temperature;
  sh:description "Property. Model:'https://schema.org/Number'. Temperature of the Pen.  Unirs:' Celsius degree'";
  sh:name "temperature";
  sh:datatype xsd:decimal
], [
  sh:path sdm:waterConsumption;
  sh:description "Property. Model:'https://schema.org/Number'. The total amount of water that came out from the tap or taps in the pen. It is measured through flowmeters and specific structure to let only one animal at a time drink.";
  sh:name "waterConsumption";
  sh:minInclusive 0;
  sh:datatype xsd:decimal
], [
  sh:path sdm:weightStDev;
  sh:description "Property. Model:'https://schema.org/Number'. The standard deviation associated to the average weight of the animals contained in the Pen.";
  sh:name "weightStDev";
  sh:datatype xsd:decimal
].
