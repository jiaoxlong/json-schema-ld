<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/dataModel.IoTDataModels/GlucoseSampleLocation/schema.json>;
    sh:property [
  sh:path sdm:rt;
  sh:name "rt";
  sh:in (sdm:oic.r.glucose.samplelocation);
  sh:maxLength 64
], [
  sh:path sdm:samplelocation;
  sh:description "Property. The possible blood locations where the blood sample may be taken.";
  sh:name "samplelocation";
  sh:in (sdm:finger sdm:ast sdm:earlobe sdm:ctrlsolution)
], [
  sh:path sdm:n;
  sh:description "Property. Friendly name of the Resource";
  sh:name "n";
  sh:maxLength 64;
  sh:datatype xsd:string
], [
  sh:path sdm:if;
  sh:name "if";
  sh:in (sdm:oic.if.r sdm:oic.if.baseline);
  sh:maxLength 64
], [
  sh:path sdm:type;
  sh:description "Property. NGSI entity type. It has to be GlucoseSampleLocation";
  sh:name "type";
  sh:in (sdm:GlucoseSampleLocation);
  sh:minCount 1;
  sh:maxCount 1
].
