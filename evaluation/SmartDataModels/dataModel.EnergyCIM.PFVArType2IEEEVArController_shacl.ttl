@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <https://w3id.org/gbfs#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix mobivoc: <http://schema.mobivoc.org/#>.
@prefix sdm: <https://w3id.org/sdm#>.
@prefix sdmshape: <https://w3id.org/sdm/shapes/schema.json#>.

<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/dataModels.CIMEnergyClasses/PFVArType2IEEEVArController/schema.json>;
    sh:property [
  sh:path sdm:type;
  sh:description "Property. NGSI type. It has to be PFVArType2IEEEVArController";
  sh:name "type";
  sh:in (sdm:PFVArType2IEEEVArController)
], [
  sh:path sdm:exlon;
  sh:description "Property. Model:'https://schema.org/Number'. Overexcitation or under excitation flag () true = 1 (not in the overexcitation or underexcitation state, integral action is active) false = 0 (in the overexcitation or underexcitation state, so integral action is disabled to allow the limiter to play its role). Default: False";
  sh:name "exlon";
  sh:datatype xsd:decimal
], [
  sh:path sdm:ki;
  sh:description "Property. Model:'https://schema.org/Number'. Integral gain of the pf controller (). Default: 0.0";
  sh:name "ki";
  sh:datatype xsd:decimal
], [
  sh:path sdm:kp;
  sh:description "Property. Model:'https://schema.org/Number'. Proportional gain of the pf controller (). Default: 0.0";
  sh:name "kp";
  sh:datatype xsd:decimal
], [
  sh:path sdm:qref;
  sh:description "Property. Model:'https://schema.org/Number'. Reactive power reference (). Default: 0.0";
  sh:name "qref";
  sh:datatype xsd:decimal
], [
  sh:path sdm:vclmt;
  sh:description "Property. Model:'https://schema.org/Number'. Maximum output of the pf controller (). Default: 0.0";
  sh:name "vclmt";
  sh:datatype xsd:decimal
], [
  sh:path sdm:vref;
  sh:description "Property. Model:'https://schema.org/Number'. Voltage regulator reference (). Default: 0.0";
  sh:name "vref";
  sh:datatype xsd:decimal
], [
  sh:path sdm:vs;
  sh:description "Property. Model:'https://schema.org/Number'. Generator sensing voltage (). Default: 0.0";
  sh:name "vs";
  sh:datatype xsd:decimal
].
