<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/dataModels.CIMEnergyClasses/WindTurbineType1or2IEC/schema.json>;
    sh:property [
  sh:path sdm:type;
  sh:description "Property. NGSI type. It has to be WindTurbineType1or2IEC";
  sh:name "type";
  sh:in (sdm:WindTurbineType1or2IEC)
], [
  sh:path sdm:WindMechIEC;
  sh:description "Property. Model:'https://schema.org/Number'. Wind mechanical model associated with this wind generator type 1 or 2 model. Default: None";
  sh:name "WindMechIEC";
  sh:datatype xsd:decimal
], [
  sh:path sdm:WindProtectionIEC;
  sh:description "Property. Model:'https://schema.org/Number'. Wind turbune protection model associated with this wind generator type 1 or 2 model. Default: None";
  sh:name "WindProtectionIEC";
  sh:datatype xsd:decimal
].
