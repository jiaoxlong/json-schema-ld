@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <https://w3id.org/gbfs#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix mobivoc: <http://schema.mobivoc.org/#>.
@prefix sdm: <https://w3id.org/sdm#>.
@prefix sdmshape: <https://w3id.org/sdm/shapes/schema.json#>.

<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/dataModels.CIMEnergyClasses/TurbineGovernorDynamics/schema.json>;
    sh:property [
  sh:path sdm:type;
  sh:description "Property. NGSI type. It has to be TurbineGovernorDynamics";
  sh:name "type";
  sh:in (sdm:TurbineGovernorDynamics)
], [
  sh:path sdm:AsynchronousMachineDynamics;
  sh:description "Property. Model:'https://schema.org/Number'. Asynchronous machine model with which this turbine-governor model is associated. Default: None";
  sh:name "AsynchronousMachineDynamics";
  sh:datatype xsd:decimal
], [
  sh:path sdm:SynchronousMachineDynamics;
  sh:description "Property. Model:'https://schema.org/Number'. Turbine-governor model associated with this synchronous machine model. Default: 'list'";
  sh:name "SynchronousMachineDynamics";
  sh:datatype xsd:decimal
], [
  sh:path sdm:TurbineLoadControllerDynamics;
  sh:description "Property. Model:'https://schema.org/Number'. Turbine load controller providing input to this turbine-governor. Default: None";
  sh:name "TurbineLoadControllerDynamics";
  sh:datatype xsd:decimal
].
