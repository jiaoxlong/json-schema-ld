@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <https://w3id.org/gbfs#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix mobivoc: <http://schema.mobivoc.org/#>.
@prefix sdm: <https://w3id.org/sdm#>.
@prefix sdmshape: <https://w3id.org/sdm/shapes/schema.json#>.

<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/dataModel.CPSV-AP/LifeEvent/schema.json>;
    sh:property [
  sh:path sdm:type;
  sh:description "Property. Model:'https://schema.org/Text'. It has to be LifeEvent.";
  sh:name "type";
  sh:in (sdm:LifeEvent);
  sh:minCount 1;
  sh:maxCount 1
], [
  sh:path sdm:identifier;
  sh:description "Property. Model:'dct:identifier'. This property represents an Identifier for the Event.";
  sh:name "identifier";
  sh:minCount 1;
  sh:maxCount 1;
  sh:datatype xsd:string
], [
  sh:path sdm:eventType;
  sh:name "eventType";
  sh:in (sdm:Having a child sdm:Becoming a (social) caretaker sdm:Starting education sdm:Looking for a new job <sdm:Losing/quitting a job> sdm:Looking for a place to live sdm:Changing relationship status sdm:Driving a vehicle sdm:Travelling abroad <sdm:Moving to/from the country> sdm:Going into military service <sdm:Facing an emergency / health problem> sdm:Facing a crime sdm:Retirement sdm:Death of a relative)
], [
  sh:path sdm:relatedService;
  sh:or ([
  sh:minLength 1;
  sh:maxLength 256;
  sh:pattern "^[\\w\\-\\.\\{\\}\\$\\+\\*\\[\\]`|~^@!,:\\\\]+$";
  sh:datatype xsd:string;
  sh:description "Property. Array of identifiers format of any NGSI entity."
] [
  sh:datatype xsd:anyURI;
  sh:description "Property. Array of URIs format of any NGSI entity."
])
].
