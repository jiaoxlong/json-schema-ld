@prefix dash: <http://datashapes.org/dash#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix schema: <https://schema.org/>.
@prefix sh: <http://www.w3.org/ns/shacl#>.
@prefix dcterms: <http://purl.org/dc/terms/>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix gbfs: <https://w3id.org/gbfs#>.
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#>.
@prefix mobivoc: <http://schema.mobivoc.org/#>.
@prefix sdm: <https://w3id.org/sdm#>.
@prefix sdmshape: <https://w3id.org/sdm/shapes/schema.json#>.

<https://w3id.org/sdm/shapes/schema.json#schema.jsonShape> rdf:type sh:NodeShape;
    sh:targetClass <https://smart-data-models.github.io/dataModels.CIMEnergyClasses/ExcIEEEST7B/schema.json>;
    sh:property [
  sh:path sdm:type;
  sh:description "Property. NGSI type. It has to be ExcIEEEST7B";
  sh:name "type";
  sh:in (sdm:ExcIEEEST7B)
], [
  sh:path sdm:kh;
  sh:description "Property. Model:'https://schema.org/Number'. High-value gate feedback gain (K).  Typical Value 1. Default: 0.0";
  sh:name "kh";
  sh:datatype xsd:decimal
], [
  sh:path sdm:kia;
  sh:description "Property. Model:'https://schema.org/Number'. Voltage regulator integral gain (K).  Typical Value = 1. Default: 0.0";
  sh:name "kia";
  sh:datatype xsd:decimal
], [
  sh:path sdm:kl;
  sh:description "Property. Model:'https://schema.org/Number'. Low-value gate feedback gain (K).  Typical Value 1. Default: 0.0";
  sh:name "kl";
  sh:datatype xsd:decimal
], [
  sh:path sdm:kpa;
  sh:description "Property. Model:'https://schema.org/Number'. Voltage regulator proportional gain (K).  Typical Value = 40. Default: 0.0";
  sh:name "kpa";
  sh:datatype xsd:decimal
], [
  sh:path sdm:oelin;
  sh:description "Property. Model:'https://schema.org/Number'. OEL input selector (OELin). Typical Value = noOELinput. Default: None";
  sh:name "oelin";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tb;
  sh:description "Property. Model:'https://schema.org/Number'. Regulator lag time constant (T).  Typical Value 1. Default: 0";
  sh:name "tb";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tc;
  sh:description "Property. Model:'https://schema.org/Number'. Regulator lead time constant (T).  Typical Value 1. Default: 0";
  sh:name "tc";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tf;
  sh:description "Property. Model:'https://schema.org/Number'. Excitation control system stabilizer time constant (T).  Typical Value 1. Default: 0";
  sh:name "tf";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tg;
  sh:description "Property. Model:'https://schema.org/Number'. Feedback time constant of inner loop field voltage regulator (T). Typical Value 1. Default: 0";
  sh:name "tg";
  sh:datatype xsd:decimal
], [
  sh:path sdm:tia;
  sh:description "Property. Model:'https://schema.org/Number'. Feedback time constant (T).  Typical Value = 3. Default: 0";
  sh:name "tia";
  sh:datatype xsd:decimal
], [
  sh:path sdm:uelin;
  sh:description "Property. Model:'https://schema.org/Number'. UEL input selector (UELin). Typical Value = noUELinput. Default: None";
  sh:name "uelin";
  sh:datatype xsd:decimal
], [
  sh:path sdm:vmax;
  sh:description "Property. Model:'https://schema.org/Number'. Maximum voltage reference signal (V).  Typical Value = 1.1. Default: 0.0";
  sh:name "vmax";
  sh:datatype xsd:decimal
], [
  sh:path sdm:vmin;
  sh:description "Property. Model:'https://schema.org/Number'. Minimum voltage reference signal (V).  Typical Value = 0.9. Default: 0.0";
  sh:name "vmin";
  sh:datatype xsd:decimal
], [
  sh:path sdm:vrmax;
  sh:description "Property. Model:'https://schema.org/Number'. Maximum voltage regulator output (V).  Typical Value = 5. Default: 0.0";
  sh:name "vrmax";
  sh:datatype xsd:decimal
], [
  sh:path sdm:vrmin;
  sh:description "Property. Model:'https://schema.org/Number'. Minimum voltage regulator output (V).  Typical Value = -4.5. Default: 0.0";
  sh:name "vrmin";
  sh:datatype xsd:decimal
].
